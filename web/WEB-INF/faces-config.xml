            <redirect />
        </navigation-case>
        <navigation-case>
            <from-outcome>dev</from-outcome>
            <to-view-id>/dashboard.xhtml</to-view-id>
            <redirect />
        </navigation-case>
        <navigation-case>
            <from-outcome>con</from-outcome>
