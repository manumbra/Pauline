        </navigation-case>
        <navigation-case>
            <from-outcome>mn</from-outcome>
            <to-view-id>/massnahmetable.xhtml</to-view-id>
            <redirect />
        </navigation-case>
        <navigation-case>
            <from-outcome>qk</from-outcome>
