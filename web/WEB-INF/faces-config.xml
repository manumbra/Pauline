    </converter>
    <converter>
        <converter-id>UserroleConverter</converter-id>
        <converter-class>
            de.affinis.pauline.converter.UserroleConverter
        </converter-class>
    </converter>
    <converter>
        <converter-id>AuftragConverter</converter-id>
